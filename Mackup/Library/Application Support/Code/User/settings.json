{
  "vscode-neovim.compositeKeys": {
    "jk": {
      // Use lua to execute any logic
      "command": "vscode-neovim.lua",
      "args": [
        [
          "local code = require('vscode')",
          "code.action('vscode-neovim.escape')"
          // "code.action('workbench.action.files.save')",
        ]
      ]
    }
  },
  "editor.fontFamily": "\"MonoLisa\", \"SFMono Nerd Font\",  \"DankMono Nerd Font\", 'monospace', monospace",
  "editor.lineHeight": 1.8,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.tabSize": 2,
  "editor.lineNumbers": "relative",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "oracledevtools.connectionConfiguration.configFilesFolder": "/home/domain80/Oracle/network/admin",
  "[oraclesql]": {
    "editor.suggest.showSnippets": true,
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    }
  },
  "oracledevtools.connectionConfiguration.walletFileFolder": "/home/domain80/Oracle/network/admin",
  "oracledevtools.bookmarkFileFolder": "/home/domain80/Oracle/oracle.oracledevtools",
  "oracledevtools.download.otherFolder": "/home/domain80/downloads",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.wordWrap": "wordWrapColumn",
  "editor.linkedEditing": true,
  "[vue]": {
    "editor.defaultFormatter": "Vue.volar"
  },
  "workbench.iconTheme": "material-icon-theme",
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "workbench.colorTheme": "Default Dark+",
  "editor.minimap.size": "fit",
  "editor.minimap.showSlider": "always",
  "editor.minimap.enabled": false,
  "editor.stickyScroll.enabled": false,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "files.refactoring.autoSave": false,
  "editor.fontLigatures": true,
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "workbench.editor.customLabels.enabled": true,
  "workbench.editor.customLabels.patterns": {
    "**/app/**/page.tsx": "${dirname}/page.tsx",
    "**/pages/**/index.vue": "${dirname}/page.vue",
    "**/index.ts": " ${dirname}/${filename}.${extname}"
  },
  "redhat.telemetry.enabled": false,
  "vscode-neovim.autoGeneratedDocumentSchemes": [
    "vscode-chat-code-block",
    "vscode-copilot-chat-code-block"
  ],
  "extensions.autoCheckUpdates": false,
  "extensions.autoUpdate": false,
  "terminal.integrated.lineHeight": 1.5,
  "terminal.integrated.fontSize": 14,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "vscode-neovim.logOutputToConsole": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.settings.applyToAllProfiles": ["vscode-neovim.logOutputToConsole"],
  "window.title": "${activeEditorMedium}${separator}${rootName}${separator}${profileName}",
  "telemetry.telemetryLevel": "off",
  "workbench.sideBar.location": "right",
  "editor.codeActionsOnSave": {
    "source.removeUnusedImports": "explicit",
    "source.organizeImports": "explicit",
    "source.addMissingImports.ts": "always"
  },
  "editor.fontSize": 13,
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "errorLens.codeLensEnabled": true,
  "errorLens.codeLensTemplate": "$count $severity $message",
  "errorLens.enabledDiagnosticLevels": ["error", "warning", "hint", "info"],
  "errorLens.exclude": ["\\/\\/ TODO:[.*]"],
  "errorLens.fontStyleItalic": true,
  "errorLens.gutterIconsEnabled": true,
  "errorLens.messageEnabled": false,
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]",
    "todo",
    "!"
  ],
  "todo-tree.highlights.customHighlight": {
    "BUG": {
      "icon": "bug"
    },
    "HACK": {
      "icon": "tools"
    },
    "FIXME": {
      "icon": "flame"
    },
    "XXX": {
      "icon": "x"
    },
    "[ ]": {
      "icon": "issue-draft"
    },
    "[x]": {
      "icon": "issue-closed"
    }
  }
}
